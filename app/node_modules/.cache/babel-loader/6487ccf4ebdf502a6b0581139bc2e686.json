,KAAqBd,OAAO,CAACc,GAAjC;AAAA,KAA5B,CAAb;;AACA,QAAGQ,MAAH,EAAW;AACTD,MAAAA,MAAM,CAAC,oBAAD,EAAuB;AAACrB,QAAAA,OAAO,EAAPA,OAAD;AAAUE,QAAAA,QAAQ,EAARA;AAAV,OAAvB,CAAN;AACA;AACD;;AACDmB,IAAAA,MAAM,CAAC,aAAD,EAAgB;AAACrB,MAAAA,OAAO,EAAPA,OAAD;AAAUE,MAAAA,QAAQ,EAARA;AAAV,KAAhB,CAAN;AACD,GARa;AASdqB,EAAAA,qBAAqB,EAAE,sCAAWP,EAAX,EAAkB;AAAA,QAAhBK,MAAgB,SAAhBA,MAAgB;AACvCA,IAAAA,MAAM,CAAC,0BAAD,EAA6BL,EAA7B,CAAN;AACD,GAXa;AAYdQ,EAAAA,iBAAiB,EAAE,kCAAWzB,IAAX,EAAoB;AAAA,QAAlBsB,MAAkB,SAAlBA,MAAkB;;AACrC,QAAId,CAAC,mCACAR,IADA;AAEHG,MAAAA,QAAQ,EAAE;AAFP,MAAL;;AAIAmB,IAAAA,MAAM,CAAC,oBAAD,EAAuBd,CAAvB,CAAN;AACD,GAlBa;AAmBdkB,EAAAA,iBAAiB,EAAE,kCAAW1B,IAAX,EAAoB;AAAA,QAAlBsB,MAAkB,SAAlBA,MAAkB;;AACrC,QAAGtB,IAAI,CAACG,QAAL,IAAiB,CAApB,EAAuB;AACrBmB,MAAAA,MAAM,CAAC,0BAAD,EAA6BtB,IAAI,CAACC,OAAL,CAAac,GAA1C,CAAN;AACA;AACD;;AACDO,IAAAA,MAAM,CAAC,oBAAD,EAAuBtB,IAAvB,CAAN;AACD;AAzBa,CAAhB;AA4BA,eAAe;AACbX,EAAAA,KAAK,EAALA,KADa;AAEbE,EAAAA,OAAO,EAAPA,OAFa;AAGbe,EAAAA,SAAS,EAATA,SAHa;AAIbc,EAAAA,OAAO,EAAPA;AAJa,CAAf","sourcesContent":["// import axios from '@/axios'\n\n\nconst state = {\n  shoppingCart: []\n}\nconst getters = {\n  shoppingCart: state => {\n    if(sessionStorage.getItem('cart') !== null) {\n      state.shoppingCart = JSON.parse(sessionStorage.getItem('cart'))\n    }\n    return state.shoppingCart\n  },\n  shoppingCartTotal: state => {\n    let total = 0\n    if(state.shoppingCart.length !== 0) {\n      state.shoppingCart.forEach(item => {\n        total += item.product.price * item.quantity\n      })\n    }\n    return total\n  },\n  shoppingCartItemCount: state => {\n    let items = 0\n    if(state.shoppingCart.length !== 0) {\n      state.shoppingCart.forEach(item => {\n        items += item.quantity\n      })\n    }\n    return items\n  }\n}\nconst mutations = {\n  ADD_TO_CART: (state, item) => {\n    let i = {\n      ...item,\n      quantity: Number(item.quantity)\n    }\n    state.shoppingCart.push(i)\n    sessionStorage.setItem('cart', JSON.stringify(state.shoppingCart))\n  },\n  // ICREMENT_QUANTITY: (state, item) => {\n  //   item.quantity += 1\n  // }\n  INCREMENT_QUANTITY: (state, {product, quantity}) => {\n    let item = state.shoppingCart.find(i => i.product._id === product._id)\n    item.quantity += Number(quantity)\n    sessionStorage.setItem('cart', JSON.stringify(state.shoppingCart))\n\n  },\n  DELETE_PRODUCT_FROM_CART: (state, id) => {\n    state.shoppingCart = state.shoppingCart.filter(item => item.product._id !== id)\n    sessionStorage.setItem('cart', JSON.stringify(state.shoppingCart))\n  },\n  DECREMENT_QUANTITY: (state, item) => {\n    item.quantity -= 1\n    sessionStorage.setItem('cart', JSON.stringify(state.shoppingCart))\n  }\n\n}\nconst actions = {\n  addProductToCart: ({commit}, {product, quantity}) => {\n    let exists = state.shoppingCart.find(item => item.product._id === product._id)\n    if(exists) {\n      commit('INCREMENT_QUANTITY', {product, quantity})\n      return\n    }\n    commit('ADD_TO_CART', {product, quantity})\n  },\n  deleteProductFromCart: ({commit}, id) => {\n    commit('DELETE_PRODUCT_FROM_CART', id)\n  },\n  incrementQuantity: ({commit}, item) => {\n    let i = {\n      ...item,\n      quantity: 1\n    }\n    commit('INCREMENT_QUANTITY', i)\n  },\n  decrementQuantity: ({commit}, item) => {\n    if(item.quantity <= 1) {\n      commit('DELETE_PRODUCT_FROM_CART', item.product._id)\n      return\n    }\n    commit('DECREMENT_QUANTITY', item)\n  }\n}\n\nexport default {\n  state,\n  getters,\n  mutations,\n  actions\n}"]}]}